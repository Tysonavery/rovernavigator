module Navigator
  class Plateu
  
    @length
    @width
   
    def initialize(messenger)
      @messenger = messenger
    end

    def length
      @length
    end
 
    def set_length(length)
      @length = length
    end
    
    def width
      @width
    end 
    
    def set_width(width)
      @width = width
    end    

  end
end
